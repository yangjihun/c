#include <stdio.h>
#include <string.h>
#define MAX 5

struct DATASET {
	char name[20];
	int age;
	char hobby[20];
};

struct INDEX {
	char name[20];
};

int copy(struct DATASET[], struct index[]);

int main() {
	struct DATASET dataset[MAX];
	struct INDEX index[MAX];
	char name[20];
	int i;
	int j;
	char n;
	for (i = 0; i < MAX; i = i + 1) {
		printf("name: ");
		scanf_s("%s", dataset[i].name, 20);
		printf("age: ");
		scanf_s("%d", &dataset[i].age);
		printf("hobby: ");
		scanf_s("%s", dataset[i].hobby, 20);
	}
	/* store into index */
	for (i = 0; i < MAX; i = i + 1) {
		dataset[i].name == index[i].name;
	}
	/* input name */
	printf("enter name: ");
	scanf_s("%s", name, 20);
	printf("\n%s\n", name);
	for (i = 0; i < MAX; i = i + 1) {
		printf("\ni\n");
		copy(&dataset[i], &index[i]);
	}
	
	//printf("%s", index[3].name);
}

int copy(struct DATASET* dataset, struct INDEX* index) {
	printf("%d", dataset.age);
}
